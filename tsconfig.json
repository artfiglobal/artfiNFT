, "components/artist/Carousel/Carousel.js", "components/artist/Carousel/Carousel.js", "components/Home2/Timer2.jsx", "components/Home2/Timer2.jsx", "components/Home2/Timer2.jsx", "components/Home2/Timer2.jsx", "components/Home2/Timer2.jsx"  ],
